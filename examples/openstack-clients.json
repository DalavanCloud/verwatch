{
    "repos": {
        "redhat-openstack": {
            "fetcher": "git",
            "options": {
                "repo_base": "https://github.com/redhat-openstack/"
            }
        },
        "openstack": {
            "fetcher": "git",
            "options": {
                "repo_base": "https://github.com/openstack/"
            }
        },
        "grizzly side repo": {
            "fetcher": "repoquery",
            "options": {
                "repo_base": "http://repos.fedorapeople.org/repos/openstack/openstack-grizzly/"
            }
        },
        "Fedora updates": { 
            "fetcher": "bodhi"
        },
        "Fedora builds": { 
            "fetcher": "koji"
        }
    },
